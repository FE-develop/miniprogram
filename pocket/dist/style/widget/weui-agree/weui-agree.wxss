.weui-agree {
  display: block;
  padding: 0.5em 30rpx;
  font-size: 26rpx;
}
.weui-agree__text {
  color: #999999;
}
.weui-agree__link {
  display: inline;
  color: #586C94;
}
.weui-agree__checkbox {
  position: absolute;
  left: -19998rpx;
}
.weui-agree__checkbox-icon {
  position: relative;
  top: 4rpx;
  display: inline-block;
  border: 2rpx solid #D1D1D1;
  background-color: #FFFFFF;
  border-radius: 6rpx;
  width: 22rpx;
  height: 22rpx;
}
.weui-agree__checkbox-icon-check {
  position: absolute;
  top: 2rpx;
  left: 2rpx;
}
